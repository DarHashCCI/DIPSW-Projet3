{% extends 'zurwan.base.html.twig' %}

{% block title %}Zurvan - dashboard{% endblock %}

{% block stylesheets %}
    <link rel="stylesheet" href="{{ asset('assets/css/dash.css') }}">
{% endblock %}

{% block body %}
    <div id="lastContacts">
        <h2>Contacts</h2>
        {% for infos in request %}
            <div class="contact">
                {% for key, info in infos %}
                    <strong>{{ key }}</strong> : {{ info }}<br>
                {% endfor %}
            </div>
        {% endfor %}
    </div>
{% endblock %}
